{
  "name": "servicebay",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "predev": "tsx scripts/pre-dev-cleanup.ts",
    "dev": "fuser -k 3000/tcp || true && tsx server.ts",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "eslint",
    "knip": "knip",
    "test": "vitest run",
    "test:agent": "chmod +x scripts/test-agent.sh && ./scripts/test-agent.sh",
    "logs": "tsx scripts/logs.ts",
    "screenshots": "node scripts/capture-screenshots.mjs",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.py": [
      "npm run test:agent"
    ]
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "@xyflow/react": "^12.10.0",
    "better-sqlite3": "^12.6.0",
    "diff": "^8.0.2",
    "elkjs": "^0.11.0",
    "jose": "^6.1.3",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.562.0",
    "mustache": "^4.2.0",
    "next": "16.1.1",
    "node-pty": "^1.1.0",
    "nodemailer": "^7.0.12",
    "prismjs": "^1.30.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "react-simple-code-editor": "^0.14.1",
    "semver": "^7.7.3",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "ssh2": "^1.17.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@next/env": "16.1.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/js-yaml": "^4.0.9",
    "@types/mustache": "^4.2.6",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.5",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/semver": "^7.7.1",
    "@types/ssh2": "^1.15.5",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "knip": "^5.81.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.49.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "knip": {
    "ignoreBinaries": [
      "fuser"
    ],
    "ignore": [
      "src/components/CheckHistoryModal.tsx"
    ]
  },
  "overrides": {
    "ellipsize": "^0.7.0",
    "react-highlight-words": {
      "react": "$react"
    }
  }
}
